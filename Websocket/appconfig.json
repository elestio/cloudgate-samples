{
  "env": "PROD",
  "version": "1.0.0",
  "title": "Cloudgate App 1",
  "description": "This is a sample cloudgate app",
  "mainDomain": "websocket-demo-1.elest.io",
  "domains": ["websocket-demo-1.elest.io", "cloudgate.appdrag.org"], 
  "publicFolder": "./public",
  "TypeAPI": "LOCAL",
  "TypeFS": "LOCAL",
  "apiEndpoints": {
    "/tests/simple" : {
      "src": "./API/tests/",
      "handler": "simple.handler"
    },
    "/tests/full" : {
      "src" : "./API/tests/",
      "handler": "full.handler"
    }
  },
  "websocketEndpoints": {
    "/echo" : {
      "src" : "./API/websocket/",
      "open": "Echo.open",
      "message": "Echo.message",
      "close": "Echo.close"
    },
    "/chat" : {
      "src" : "./API/websocket/",
      "open": "Chat.open",
      "message": "Chat.message",
      "close": "Chat.close"
    }
  }
}